{
    "extends": "airbnb-base",
    "rules": {
        no-param-reassign: 0,
        no-shadow: 0,
        no-underscore-dangle: 0,
        prefer-destructuring: 0,
        no-restricted-globals: 0,
        class-methods-use-this: 0,
        consistent-return: 0,
    },
    "globals": {
        document: true,
        Element: true
    }
}
