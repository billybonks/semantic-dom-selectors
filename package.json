{
  "name": "semantic-dom-selectors",
  "version": "0.1.0",
  "description": "Select dom elements using html semantics instead of css",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint:js": "eslint .",
    "build": "rollup -c",
    "prepublish": "rollup -c",
    "docs": "documentation build src -f md >> API.md",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watchAll --notify",
    "releaseit": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tradegecko/semantic-dom-selectors"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aria-api": "^0.3.0",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "rollup": "^0.61.2",
    "rollup-plugin-babel": "^3.0.5"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^1.1.0",
    "babel-jest": "^23.4.2",
    "eslint": "^4.19.1",
    "documentation": "^12.1.4",
    "eslint-config-airbnb-base": "13.0.0",
    "eslint-plugin-import": "^2.12.0",
    "jest": "^23.4.2",
    "jest-runner-eslint": "^0.6.0",
    "release-it": "^12.4.3"
  },
  "jest": {
    "verbose": true,
    "projects": [
      {
        "testURL": "http://localhost/",
        "displayName": "test"
      },
      {
        "displayName": "lint",
        "runner": "jest-runner-eslint",
        "testMatch": [
          "<rootDir>/**/*.js"
        ]
      }
    ]
  }
}
